	CreateCMD({
		Name = "arrest",
		CallBack = function(Args)
			local origPos =game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
			local Arg1 = Args[1]
			local Arg2 = Args[2]

			local FoundPlayer = FindPlayer(Arg2) --> nil (if not found) OR --> player model (if found)

			if FoundPlayer then
				local startTime = tick() -- record the start time of the loop
				while tick() - startTime < 5 do -- loop for 5 seconds
					wait(0.1)
					Player.Character:SetPrimaryPartCFrame(FoundPlayer.Character:GetPrimaryPartCFrame())

					game.Workspace.Remote.arrest:InvokeServer(FoundPlayer.Character.HumanoidRootPart)

					-- Check if the player is successfully arrested
					if FoundPlayer.Character.HumanoidRootPart.Position.Y < 0 then
						-- Stop the loop
						break
					end
				end

				-- Print a message indicating that the player has been arrested
				print("Player " .. Arg2 .. " has been successfully arrested!")
				game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = origPos
			end


		end,
	})
