function guns()
    
    --ak
			  
            workspace.Remote.ItemHandler:InvokeServer({
            Position = game.Players.LocalPlayer.Character.Head.Position,
            Parent = workspace.Prison_ITEMS:FindFirstChild("AK-47", true)
})
 
--shotgun
             workspace.Remote.ItemHandler:InvokeServer({
            Position = game.Players.LocalPlayer.Character.Head.Position,
            Parent = workspace.Prison_ITEMS:FindFirstChild("Remington 870", true)
})
		 
		 --m9
		 			
		 	workspace.Remote.ItemHandler:InvokeServer({
            Position = game.Players.LocalPlayer.Character.Head.Position,
            Parent = workspace.Prison_ITEMS:FindFirstChild("M9", true)
}) 
--m4
			workspace.Remote.ItemHandler:InvokeServer({
            Position = game.Players.LocalPlayer.Character.Head.Position,
            Parent = workspace.Prison_ITEMS:FindFirstChild("M4A1", true)
})
	
	 
    end
 
 

local Player = game:GetService("Players").LocalPlayer
FindPlayer = function(h,h2)
	if string.lower(h) == "me" then
		return Player
	else
		h = h:gsub("%s+", "")
		for m, n in pairs(game:GetService("Players"):GetPlayers()) do
			if n.Name:lower():match("^" .. h:lower()) or n.DisplayName:lower():match("^" .. h:lower()) then
				return n
			end
		end
	end
	return nil
end




function BringPlayer(Target,BringPosition) --//This has to be an instance (ex: game.Players.ExamplePlayer)
	--//BringPosition is you can bring the player to a cframe position
	local OldTeam = game:GetService("Players").LocalPlayer.TeamColor.Name
	local SavedPosition = game:GetService("Players").LocalPlayer.Character:GetPrimaryPartCFrame()--//Get your characters position
	if not BringPosition then
		BringPosition = SavedPosition
	end
	
	workspace.Remote.TeamEvent:FireServer("Bright blue") --//Make your team into guards
	
	wait(.2)
	
	game:GetService("Players").LocalPlayer.Character:SetPrimaryPartCFrame(SavedPosition)
	
	wait()
	
	local TargetToBring = Target
	local Character = game:GetService("Players").LocalPlayer.Character --// your character 
	local ToolInBackpack = game:GetService("Players").LocalPlayer.Backpack:FindFirstChild("Handcuffs") --//Gets tool from backpack
	Character:FindFirstChildOfClass("Humanoid"):Destroy() --//Finds the humanoid and destroys it
	Instance.new("Humanoid",Character) --//Puts a new humanoid in your character
	workspace.CurrentCamera.CameraSubject = Character--//makes your camera focus your character
	ToolInBackpack.Parent = Character --//Puts tool in your character
	repeat task.wait() ---//Waits until your tool is not in your character
		Character:SetPrimaryPartCFrame(BringPosition)--//Loops you to one position
		TargetToBring.Character:SetPrimaryPartCFrame(Character:GetPrimaryPartCFrame()) --//Loops player to you
		if firetouchinterest then
			--//Pcall means if the function errors it will be silent and not break the script but also not show any errors
			pcall(function()--firetouchinterest exploit function use it like this firetouchinterest(<Touching part>,<PartThatsGettingTouched,0) --// 0 means touch 1 mean untouch
				firetouchinterest(TargetToBring.Character.Head,ToolInBackpack.Handle,0)
				firetouchinterest(TargetToBring.Character.Head,ToolInBackpack.Handle,1)
			end)
		end
		
	until ToolInBackpack.Parent ~= Character or not Character or not ToolInBackpack or not TargetToBring.Character or TargetToBring.Character:FindFirstChild("Handcuffs") --//If tool is not in your character
	local Connection = nil
	wait(.25)
	Connection=game:GetService("Players").LocalPlayer.CharacterAdded:Connect(function(newCharacter)--//connection
		repeat task.wait() until newCharacter and newCharacter:FindFirstChildOfClass("Humanoid")
		for i =1,2 do --//Loop  2 times and stop
			newCharacter:SetPrimaryPartCFrame(SavedPosition)
			task.wait() --//Wait but faster
		end
		if OldTeam == "Really red" then --//Make you a criminal by touching the pad
			wait(.3)
			local CriminalsPad = workspace["Criminals Spawn"].SpawnLocation
			local Oldp = CriminalsPad.CFrame
			repeat task.wait()CriminalsPad.CFrame = game:GetService("Players").LocalPlayer.Character:GetPrimaryPartCFrame() until game:GetService("Players").LocalPlayer.Team == game.Teams.Criminals
			wait()
			CriminalsPad.CFrame = Oldp
		end
		Connection:Disconnect() --//Stop the connection
	end)
	if OldTeam ~= "Really red" then
		workspace.Remote.TeamEvent:FireServer(OldTeam) --//Make your team into last one
	end
end


task.spawn(function()
	local popup = Instance.new("ScreenGui")
	popup.Name = "WelcomePopup"
	popup.ResetOnSpawn = false


	local message = Instance.new("TextLabel")
	message.Name = "Message"
	message.Size = UDim2.new(0, 300, 0, 50)
	message.Position = UDim2.new(0.5, -150, 0.5, -25)
	message.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	message.BackgroundTransparency = 1
	message.TextColor3 = Color3.fromRGB(255, 255, 255)
	message.TextTransparency = 1
	message.TextScaled = true
	message.Font = Enum.Font.SourceSansBold
	message.Text = "Welcome to Raisen admin!"
	message.Parent = popup

	popup.Parent = game.Players.LocalPlayer.PlayerGui


	local tweenInInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
	local tweenIn = game:GetService("TweenService"):Create(message, tweenInInfo, {BackgroundTransparency = 1, TextTransparency = 0})
	tweenIn:Play()


	wait(2)


	local tweenOutInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	local tweenOut = game:GetService("TweenService"):Create(message, tweenOutInfo, {Position = UDim2.new(0.5, -150, 0, -50), BackgroundTransparency = 1, TextTransparency = 1})
	tweenOut:Play()


	tweenOut.Completed:Connect(function()
		popup:Destroy()
	end)

	wait(2)

	local popup = Instance.new("ScreenGui")
	popup.Name = "WelcomePopup"
	popup.ResetOnSpawn = false


	local message = Instance.new("TextLabel")
	message.Name = "Message"
	message.Size = UDim2.new(0, 300, 0, 50)
	message.Position = UDim2.new(0.5, -150, 0.5, -25)
	message.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	message.BackgroundTransparency = 1
	message.TextColor3 = Color3.fromRGB(255, 255, 255)
	message.TextTransparency = 1
	message.TextScaled = true
	message.Font = Enum.Font.SourceSansBold
	message.Text = "Raisen"
	message.Parent = popup

	popup.Parent = game.Players.LocalPlayer.PlayerGui


	local tweenInInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.In)
	local tweenIn = game:GetService("TweenService"):Create(message, tweenInInfo, {BackgroundTransparency = 1, TextTransparency = 0})
	tweenIn:Play()


	wait(2)


	local tweenOutInfo = TweenInfo.new(1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out)
	local tweenOut = game:GetService("TweenService"):Create(message, tweenOutInfo, {Position = UDim2.new(0.5, -150, 0, -50), BackgroundTransparency = 1, TextTransparency = 1})
	tweenOut:Play()


	tweenOut.Completed:Connect(function()
		popup:Destroy()
	end)

	local popup = Instance.new("ScreenGui")
	popup.Name = "WelcomePopup"
	popup.ResetOnSpawn = false


	local message = Instance.new("TextLabel")
	message.Name = "Message"
	message.Size = UDim2.new(0, 300, 0, 50)
	message.Position = UDim2.new(0.5, -150, 0.5, -25)
	message.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
	message.BackgroundTransparency = 1
