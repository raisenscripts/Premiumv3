					newDoor.Parent = game.Workspace.Doors
				end
			end
		end,
	})


	local isSpinning = false
	local spinSpeed = 2000

	CreateCMD({
		Name = "spin",
		CallBack = function(Args)
			if isSpinning then
				print("You are already spinning!")
				return
			end
			isSpinning = true

			while isSpinning do
				Player.Character:SetPrimaryPartCFrame(Player.Character:GetPrimaryPartCFrame() * CFrame.Angles(0, math.rad(spinSpeed), 0)) -- Rotate the player's CFrame
				wait() -- Wait for the next frame
			end
		end,
	})

	CreateCMD({
		Name = "unspin",
		CallBack = function(Args)
			if not isSpinning then
				print("You are not currently spinning!")
				return
			end
			isSpinning = false
			-- Teleport the player back to their original position
		end,
	})
	
	
