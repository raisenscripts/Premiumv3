	CreateCMD({
		Name = "gmod",
		CallBack = function(Args)
			local LocalPlayer = Players.LocalPlayer
			local Backpack = LocalPlayer.Backpack
			while true do
				for _, Gun in ipairs(Backpack:GetChildren()) do
					local GunStates = Gun:FindFirstChild("GunStates")
					if GunStates then
						GunStates = require(GunStates)
						GunStates.AutoFire = true
						GunStates.FireRate = 0.01
						GunStates.Spread = 0
						GunStates.Bullets = 1 --// Don't do this too high
					end
				end
				wait(.5) 
			end
		end,
	})
	 
	CreateCMD({
		Name = "arrestall",
		CallBack = function(Args)
			wait(0.1)
			Player = game.Players.LocalPlayer
			Pcf = Player.Character.HumanoidRootPart.CFrame
			for i,v in pairs(game.Teams.Criminals:GetPlayers()) do
				if v.Name ~= Player.Name then
					local i = 13
					repeat
						wait()
						i = i-1
						game.Workspace.Remote.arrest:InvokeServer(v.Character.HumanoidRootPart)
						Player.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1)
					until i == 0
				end
			end
		end,
	})
	 local LocalPlayer = game:GetService("Players").LocalPlayer
local LoopKilling = {}

 

game:GetService("RunService").Heartbeat:Connect(function()
	for i, Player in pairs(LoopKilling) do
		if LocalPlayer.Character:FindFirstChild("Head") and Player and Player.Character:FindFirstChild("Head") then
			if not table.find(excludedPlayerIds, Player.UserId) and not table.find(whitelist, Player.UserId) then 
				LocalPlayer.Character:SetPrimaryPartCFrame(Player.Character.Head.CFrame)
				for i = 1, 20 do
					game:GetService("ReplicatedStorage").meleeEvent:FireServer(Player)
				end
			end
		end
	end
end)

 CreateCMD({
    Name = "killall",
    CallBack = function(Args)
        local origPos = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame
        local players = game:GetService("Players")
        local localPlayer = players.LocalPlayer
        local killedPlayers = {}
         

        spawn(function()
            while true do
                local allDead = true
                for i, v in ipairs(players:GetPlayers()) do
                    if not table.find(whitelist, v.UserId) and v ~= localPlayer and v.Character and v.Character:FindFirstChildOfClass("Humanoid") and v.Character.Humanoid.Health > 0 then
                        if not killedPlayers[v.UserId] then
                            local team = v.TeamColor
                            if team ~= nil and team.Name ~= "Medium stone grey" then
                                allDead = false
                                local vHumanoidRootPart = v.Character:WaitForChild("HumanoidRootPart")
                                local localHumanoidRootPart = localPlayer.Character:WaitForChild("HumanoidRootPart")
                                while v.Character:WaitForChild("Humanoid").Health > 0 do
                                    wait()
                                    localHumanoidRootPart.CFrame = vHumanoidRootPart.CFrame
                                    for i = 1, 13 do
                                        game:GetService("ReplicatedStorage").meleeEvent:FireServer(v)
                                    end
                                    killedPlayers[v.UserId] = true
                                end
                            end
                        end
                    end
                end
                if allDead then
                    game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = origPos
                    break
                end
                wait(0.1)
            end
        end)
    end,
})


	CreateCMD(
		{
			Name = "lk",
			CallBack = function(Args)
				local Arg1 = Args[1]
				local Arg2 = Args[2]
				local FoundPlayer = FindPlayer(Arg2)
				if FoundPlayer then
					table.insert(LoopKilling, FoundPlayer)
				end
			end
		}
	)

	CreateCMD(
		{
			Name = "unlk",
			CallBack = function(Args)
				local Arg1 = Args[1]
				local Arg2 = Args[2]
				local FoundPlayer = FindPlayer(Arg2)
				if FoundPlayer then
					for i, Player in ipairs(LoopKilling) do
						if Player == FoundPlayer then
							table.remove(LoopKilling, i)
							break
						end
					end
				end
			end
		}
	)

