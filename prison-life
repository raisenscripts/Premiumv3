	local whitelist = {}

CreateCMD({
    Name = "whitelist",
    CallBack = function(Args)
        local Arg1 = Args[1]
        local Arg2 = Args[2]
        local FoundPlayer = FindPlayer(Arg2)
        if FoundPlayer then
            table.insert(whitelist, FoundPlayer.UserId)
            print("Added player " .. FoundPlayer.Name .. " to the whitelist.")
        else
            print("Could not find player " .. Arg2 .. ".")
        end
    end,
})

CreateCMD({
    Name = "unwhitelist",
    CallBack = function(Args)
        local Arg1 = Args[1]
        local Arg2 = Args[2]
        local FoundPlayer = FindPlayer(Arg2)
        if FoundPlayer then
            for i, UserId in ipairs(whitelist) do
                if UserId == FoundPlayer.UserId then
                    table.remove(whitelist, i)
                    print("Removed player " .. FoundPlayer.Name .. " from the whitelist.")
                    return
                end
            end
            print("Player " .. FoundPlayer.Name .. " is not in the whitelist.")
        else
            print("Could not find player " .. Arg2 .. ".")
        end
    end,
})


